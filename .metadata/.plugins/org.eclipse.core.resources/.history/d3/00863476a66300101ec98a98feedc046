package hello_java_world;

public class ExamDiv {
	public static void main(String[] args) {
		int a = 1;
		int b = 3;
		double result = a / (double) b;
		System.out.println(result);
		
		// 프로그래머스
		int num1 = 3;
		int num2 = 2;
		int num_result = num1 % num2;
		System.out.println(num_result);
		
		int num1_ = 3;
		int num2_ = 2;
		int num_result_ = num1_ / num2_;
		System.out.println(num_result_);
	}

}
