package hello_java_world;

public class ArtithmaticProblem2 {
	public static void main(String[] args) {
		// 문제 2 :
		int processTime = 145;
		int minutes = 0;
		int seconds = 0;
		
		minutes = processTime / 60;
		seconds = processTime % 60;
		
		System.out.println(minutes + " 분 " + seconds + " 초" );


	}

}
