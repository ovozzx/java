package _250721;

public class Practice2 {
	public static void main(String[] args) {
		System.out.println("직업 입력하는 계산기");
		
		int numberOne = 10;
		int numberTwo = 20;
		
		System.out.println(numberOne + numberTwo);
		System.out.println(numberOne - numberTwo);
		System.out.println(numberOne * numberTwo);
		System.out.println(numberOne / numberTwo);
	}
}
