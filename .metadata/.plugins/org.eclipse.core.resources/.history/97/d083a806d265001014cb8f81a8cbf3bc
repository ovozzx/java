package _250721;

import java.util.Scanner;

public class Practice2 {
	public static void main(String[] args) {
		
		System.out.println("직업 입력하는 계산기");
	
		// 키보드의 입력을 받아들이는 코드
		Scanner consoleInput = new Scanner(System.in);
		
		//int numberOne = 10; // hard coding
		//int numberTwo = 20;
		int numberOne = consoleInput.nextInt();
		int numberTwo = consoleInput.nextInt();
		
		String operator = "+"; // 문자열 작은 따옴표 안됨 
		
		if(operator == "+") {
			System.out.println(numberOne + numberTwo); // alt 키 누른채로 방향키로 이동 가능 (mac은 option 키로)
		}
		else if(operator == "-") {
			System.out.println(numberOne - numberTwo);			
		}
		else if(operator == "*") {
			System.out.println(numberOne * numberTwo);			
		}
		else {
			System.out.println(numberOne / numberTwo);			
		}
	}
}
