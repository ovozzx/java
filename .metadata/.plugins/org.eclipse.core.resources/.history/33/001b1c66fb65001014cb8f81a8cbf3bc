package _250721;

public class ArrayExam {
	
	public static void main(String[] args) {
		
		// 일반 변수는 메모리에 랜덤하게 지정됨
		// 배열은 메모리에 연결되어 지정됨 (메모리 주소가 연결되어 있음)
		int[] students = new int[5]; // 변수 배열을 줄 지어서 새롭게 만들어라
		students[0] = 100;
		students[1] = 200;
		students[2] = 300;
		students[3] = 100;
		students[4] = 200;
		
		int sum = students[0] + students[1] + students[2] + students[3] + students[4];
	}

}
