package _250721;

public class Problem162 {
	public static void main(String[] args) {
		
		
		// 1번
		int sum = 0;
		for(int i = 1 ; i <= 100 ;i++) {
			sum += i;
		}
		System.out.println(sum);
		
		// 2번
		int sum2 = 0;
		for(int i = 1 ; i <= 100 ;i++) {
			if(i % 2 != 0) {
				sum2 += i;
			}
			
		}
		System.out.println(sum2);		
		
		// 4번
		String star = "";
		for(int i = 0 ; i < 5 ; i++) {
			star += "*";
			System.out.println(star);
		}
	}

}
