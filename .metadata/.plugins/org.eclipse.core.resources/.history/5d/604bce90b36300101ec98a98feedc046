package hello_java_world;


public class HelloJava {
	
	public static void main(String[] args) {
		
		System.out.println("Hello, Java World!");
		System.out.println(10);
		System.out.println(10+30+50);
		//System.out.println("안녕하세요! 제 이름은 "이소영"입니다."); 에러 코드
		System.out.println("안녕하세요! 제 이름은 \"이소영\"입니다.");
		System.out.println("사는 곳은 \"인천\"입니다.");
		System.out.println("\\");
		
		/*
		 * 큰 따옴표 안의 기호 " \ 들은 \를 앞에 붙여서 출력한다
		 * \ <== Escape Code
		 *
		 * */
	}
	
}
